<div class='tree-wrapper' >
  <ul id="myTree" data-toggle="nav-tree" >
    <%= tree_for(Place.arrange) %>
   </ul>
</div>

<div class='tree-action' >
  <%= link_to '#', class: 'new-tree' do %>
    <i class='glyphicon glyphicon-plus'> </i>
  <% end %>
  <%= link_edit '', '#', class: 'edit-tree' %>
  <%= link_destroy '', '#', class: 'destroy-tree', method: :delete %>
</div>